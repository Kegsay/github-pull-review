{
  "name": "github-pull-review",
  "version": "0.1.0",
  "description": "A better UI for doing Github Pull Requests",
  "main": "index.js",
  "dependencies": {
    "babel-cli": "^6.1.1",
    "babel-plugin-transform-class-properties": "^6.0.14",
    "babel-plugin-transform-flow-strip-types": "^6.0.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "bluebird": "^2.10.0",
    "browserify": "^11.1.0",
    "flux": "^2.1.1",
    "highlight.js": "^8.8.0",
    "http-server": "^0.8.0",
    "marked": "^0.3.5",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.0-rc3",
    "react-tools": "^0.13.3",
    "request": "^2.62.0",
    "shelljs": "^0.5.3"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "eslint": "^1.5.1",
    "eslint-plugin-react": "^3.4.2",
    "flow-bin": "^0.18.1",
    "parallelshell": "^2.0.0",
    "watchify": "^3.4.0"
  },
  "scripts": {
    "build": "babel lib/ --out-dir gen/ && browserify -e gen/app.js -o build/bundle.js && shjs build.js",
    "test": "npm run typecheck && npm run build && npm run lint",
    "lint": "eslint lib tests/ui",
    "typecheck": "flow check lib/",
    "watch": "parallelshell \"babel --presets es2015,react --watch lib/ --out-dir gen/\" \"watchify -e gen/app.js -o build/bundle.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kegsay/github-pull-review.git"
  },
  "keywords": [
    "github",
    "pull-requests",
    "pr",
    "code-review"
  ],
  "author": "Kegan Dougal",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Kegsay/github-pull-review/issues"
  },
  "homepage": "https://github.com/Kegsay/github-pull-review#readme"
}
